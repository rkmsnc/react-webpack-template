{
  "name": "react-webpack-template",
  "version": "1.0.0",
  "description": "A React Webpack App Template.",
  "main": "main.js",
  "scripts": {
    "start:dev": "nodemon --watch ./webpack.config.js --exec 'webpack serve --config ./webpack.config.js'",
    "lint": "eslint ./src/**/*.jsx",
    "start": "webpack serve",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production"
  },
  "keywords": [
    "react",
    "react-dom",
    "webpack",
    "eslint",
    "prettier"
  ],
  "author": "Radha Krishna <radhakrishnabca@gmail.com> (https://github.com/rkmsnc)",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
